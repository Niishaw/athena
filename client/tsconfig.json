{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable"
      ],
      "jsx": "react-jsx",
      "module": "esnext",
      "moduleResolution": "node",
      "baseUrl": "src",
      "typeRoots": [
              "./node_modules/@types",
        "./types"
      ],
      "resolveJsonModule": true,
      "allowJs": true,
      "outDir": "./build",
      "removeComments": false,
      "noEmit": true,
      "isolatedModules": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": false,
      "noFallthroughCasesInSwitch": true,
      "skipLibCheck": true
    },
    "include": [
      "src"
    ],
    "exclude": [
      "./build/*"
    ]
  }
  